<!DOCTYPE html><html lang="en"><head><meta charset="utf-8"><meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no"><title>e5n</title><link rel="stylesheet" href="assets/bootstrap/css/bootstrap.min.css"><link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Alata&amp;display=swap"><link rel="stylesheet" href="assets/fonts/font-awesome.min.css"><link rel="stylesheet" href="assets/css/styles.min.css">

<script>

var json = <%- JSON.stringify(data) %>;

function loadTable(){
var table = document.getElementById('pontok').getElementsByTagName('tbody')[0];

var arrayLength = json.length;
for (var i = 0; i < arrayLength; i++) {
    table.innerHTML += `<tr style="margin: 0px;padding: 9px;padding-top: 9px;"><td style="margin: 0px;padding: 9px;padding-top: 9px;">${json[i].name}</td><td style="margin: 0px;padding: 9px;padding-top: 9px;">${json[i].score}<br></td></tr>`;
}
}

function sort(){
var prog = []
var table = document.getElementById('pontok').getElementsByTagName('tbody')[0];
table.innerHTML = ``;
var arrayLength = json.length;
for (var i = 0; i < arrayLength; i++) {
    if (json[i].terem == false) {
    prog.push(json[i])
    }
}

prog.sort((elso, masodik) => masodik.score - elso.score);
var arrayLength = prog.length;
for (var i = 0; i < arrayLength; i++) {
    table.innerHTML += `<tr style="margin: 0px;padding: 9px;padding-top: 9px;"><td style="margin: 0px;padding: 9px;padding-top: 9px;">${prog[i].name}</td><td style="margin: 0px;padding: 9px;padding-top: 9px;">${prog[i].score}<br></td></tr>`;
}
}

function tsort(){
var prog = []
var table = document.getElementById('pontok').getElementsByTagName('tbody')[0];
table.innerHTML = ``;
var arrayLength = json.length;
for (var i = 0; i < arrayLength; i++) {
    if (json[i].terem == true) {
    prog.push(json[i])
    }
}

prog.sort((elso, masodik) => masodik.score - elso.score);
var arrayLength = prog.length;
for (var i = 0; i < arrayLength; i++) {
    table.innerHTML += `<tr style="margin: 0px;padding: 9px;padding-top: 9px;"><td style="margin: 0px;padding: 9px;padding-top: 9px;">${prog[i].name}</td><td style="margin: 0px;padding: 9px;padding-top: 9px;">${prog[i].score}<br></td></tr>`;
}
}


</script>
